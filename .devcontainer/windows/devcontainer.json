{
    "name" : "ROV Windows",
    "image": "ghcr.io/cwrurobotics/rov-24:docker",
    "containerEnv": {
        "DISPLAY": "${localEnv:DISPLAY}"
    },
    "mounts": [
        "type=bind,source=${localEnb:HOME}\\${localEnv:USER}.ssh,target=/root/.ssh,readonly"
      ],
    "workspaceFolder": "/rov-24",
    "customizations": {
        "vscode" : {
            "extensions": ["ms-python.flake8", "ms-azuretools.vscode-docker", "streetsidesoftware.code-spell-checker"]
        }
    },
    "privileged": true
}