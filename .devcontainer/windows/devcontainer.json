{
    "name": "ROV Windows",
    "image": "ghcr.io/cwrurobotics/rov-24:iron",
    "containerEnv": {
        "DISPLAY": "${localEnv:DISPLAY}"
    },
    "workspaceFolder": "/root/rov-24",
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-python.flake8",
                "ms-azuretools.vscode-docker",
                "streetsidesoftware.code-spell-checker",
                "ms-python.python",
                "ms-python.mypy-type-checker"
            ]
        }
    },
    "privileged": true
}